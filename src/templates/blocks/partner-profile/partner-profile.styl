// autocomplete
.autocomplete__wrap
    position relative 
    width 100%
.ui-autocomplete
    border: 1px solid rgba(22,141,164,.7)
    background-color #fff
    z-index 2 
    width 200px
    .ui-menu-item
        cursor: pointer
        font-size: 14px
        padding: 5px 10px 5px 20px
        &:hover
            background-color #e5f2f5
            color #168da4

// partner-profile_vidjets
.vidjets-block
    display inline-flex
    flex-flow: row wrap
    width 100.01%  
    &__item
        position relative
        width 33.333333333%
        height 186px  
        border-right 1px dashed rgba(29,137,160,.45)  
        box-sizing border-box
        text-align center
        transition all 0.3s
        outline none
        &:nth-child(3n)
            border-right none
        &:nth-child(-n+3)
            border-bottom 1px dashed rgba(29,137,160,.45)
        &:hover
            background green_base
        &:hover .vidjets-block__text
            color #FFF 
        &:hover .vidjet-doctor
            &:before
                background url('i/icons/vidjet-doctor_hover.png')  no-repeat
        &:hover .vidjet-btn
            &:before
                background url('i/icons/vidjet-btn_hover.png')  no-repeat
        &:hover .vidjet-clinic
            &:before
                background url('i/icons/vidjet-clinic_hover.png')  no-repeat
        // &:hover .vidjets-doctors
        //     &:before 
        //         background url('i/icons/vidjets-doctors_hover.png')  no-repeat
        // &:hover .vidjets-clinics
        //     &:before
        //         background url('i/icons/vidjets-clinics_hover.png')  no-repeat
        &:hover .vidjet-search
            &:before
                background url('i/icons/vidjet-search_hover.png')  no-repeat
                
     
    &__link
        display block
        width 100%
        height 100%
        text-decoration none
        vertical-align middle
        &:focus
            text-decoration none
            outline none 
    &__text
        display block
        color #1d89a0
        font-size 22px
        font-weight 700
        margin-top 15px
        
        
    &__picture
        display block
        &:before
            content ''
            display block
            margin 0 auto
            padding-top 20px 
        &.vidjet-btn
            padding-top 53px
            &:before
                background url('i/icons/vidjet-btn.png')  no-repeat
                width 153px
                height 53px
        &.vidjet-doctor
            padding-top 40px
            &:before
                background url('i/icons/vidjet-doctor.png')  no-repeat
                width 146px
                height 66px
        &.vidjet-clinic
            padding-top 40px
            &:before
                background url('i/icons/vidjet-clinic.png')  no-repeat
                width 146px
                height 66px
        &.vidjets-doctors
            padding-top 40px
            &:before
                background url('i/icons/vidjets-doctors.png')  no-repeat
                width 153px
                height 74px
        &.vidjets-clinics
            padding-top 40px
            &:before
                background url('i/icons/vidjets-clinics.png')  no-repeat
                width 149px
                height 70px
        &.vidjet-search
            padding-top 64px
            &:before
                background url('i/icons/vidjet-search.png')  no-repeat
                width 215px
                height 46px
    
.come-back
    margin-top 20px
    &__link
        color #1d89a0
        font-size 16px
        font-weight 400
        text-decoration none
        border-bottom 1px dotted #1d89a0
        margin-left 8px 
        &:hover
            border-color transparent
        &:focus
            outline none
            text-decoration none
    &__icon
        display inline-block
        margin-top -4px
        vertical-align middle
        background url('i/icons/profile-back-arrow.png')  no-repeat
        width 13px
        height 13px


// partner-profile_vidjets-preview
.setting-element
    &__content
        padding  10px 20px 10px 20px
        .installation
            width 100%
            height 300px
            border-collapse collapse
            tr
                td
                    
                    &:first-child
                        width 25%
                        font-size 15px
                        color text_color
                    &:last-child
                        width 75%
                    .profile
                        .form-radio
                            float none
                            width auto
                            label
                                margin-bottom 0
                    .enter-info
                        background-color: white
                        border 1px solid rgba(22, 141, 164, 0.7)
                        border-radius 3px
                        padding-left 18px
                        color #91b5bc
                        width 100%
                        height 31px
                        margin 0
                        placeholder(#91b5bc, 14)
    &__footer
        text-align center
        background-color rgba(#eaf5f9, 0.48)
        border-top 1px dashed rgba(29,137,160,.45)
        padding 24px 0
    &__button
        width 218px
        background-color #e74a37 
        text-shadow 0.5px 0.9px 0 rgba(48, 49, 49, 0.23)
        color white
        font-size 18px
        font-weight 700
        padding 8px 0 
        &:hover
            background-color #D84C3B  
            color white 
        &:focus
            outline none
            color white 
        &:active
            outline none

.js-section-choice
    position relative
    .setting-element__field-text
        padding-top 4px
        &:after
            content ''
            float right
            margin 8px 6px 0 0
            display inline-block
            border 3px solid transparent
            border-top 6px solid #1d89a0
    .section-choice
        display none
        position absolute
        top 31px 
        z-index 10
        background-color #FFF  
        right 0
        border 1px solid rgba(22, 141, 164, 0.7)
        &.is-open
            display block
            width 100% 
        .everyone-lists__list
            cursor pointer
            font-size 14px
            padding 5px 10px 5px 20px
            &:hover
                background-color #e5f2f5
                color #168da4      
