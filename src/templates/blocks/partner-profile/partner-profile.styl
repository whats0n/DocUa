.color-block
    position relative
    display inline-block
    width 130px
    height 31px
    background-color white
    border 1px dashed #1d89a0
    border-radius 3px
    box-shadow 0 2px 4px rgba(0, 64, 76, 0.15)
    padding 5px 5px 5px 40px
    color #91b5bc
    font-size 18px 
    line-height 20px
    @media only screen and (max-width: 1200px)
        width 110px
    &__color
        position absolute
        left 5px
        top 50%
        margin-top -10px
        width 20px
        height 20px
        border-radius 3px
        border  1px solid rgba(22, 141, 164, 0.7)
        cursor pointer
.next-color
    display inline
    float right
    font-size 15px
    @media only screen and (max-width: 991px)
        float none
        margin-left 28px
    span
        @media only screen and (max-width: 1070px) 
            display none
    .color-block
        margin-left 15px 

