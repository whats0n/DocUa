.org/wiki/css/atrules/@supports\",\"title\":\"WebPlatform Docs\"}],\"categories\":[\"CSS3\"],\"stats\":{\"ie\":{\"5.5\":\"n\",\"6\":\"n\",\"7\":\"n\",\"8\":\"n\",\"9\":\"n\",\"10\":\"n\",\"11\":\"n\"},\"edge\":{\"12\":\"y\",\"13\":\"y\"},\"firefox\":{\"2\":\"n\",\"3\":\"n\",\"3.5\":\"n\",\"3.6\":\"n\",\"4\":\"n\",\"5\":\"n\",\"6\":\"n\",\"7\":\"n\",\"8\":\"n\",\"9\":\"n\",\"10\":\"n\",\"11\":\"n\",\"12\":\"n\",\"13\":\"n\",\"14\":\"n\",\"15\":\"n\",\"16\":\"n\",\"17\":\"n\",\"18\":\"n\",\"19\":\"n\",\"20\":\"n\",\"21\":\"n\",\"22\":\"y\",\"23\":\"y\",\"24\":\"y\",\"25\":\"y\",\"26\":\"y\",\"27\":\"y\",\"28\":\"y\",\"29\":\"y\",\"30\":\"y\",\"31\":\"y\",\"32\":\"y\",\"33\":\"y\",\"34\":\"y\",\"35\":\"y\",\"36\":\"y\",\"37\":\"y\",\"38\":\"y\",\"39\":\"y\",\"40\":\"y\",\"41\":\"y\",\"42\":\"y\",\"43\":\"y\",\"44\":\"y\"},\"chrome\":{\"4\":\"n\",\"5\":\"n\",\"6\":\"n\",\"7\":\"n\",\"8\":\"n\",\"9\":\"n\",\"10\":\"n\",\"11\":\"n\",\"12\":\"n\",\"13\":\"n\",\"14\":\"n\",\"15\":\"n\",\"16\":\"n\",\"17\":\"n\",\"18\":\"n\",\"19\":\"n\",\"20\":\"n\",\"21\":\"n\",\"22\":\"n\",\"23\":\"n\",\"24\":\"n\",\"25\":\"n\",\"26\":\"n\",\"27\":\"n\",\"28\":\"y\",\"29\":\"y\",\"30\":\"y\",\"31\":\"y\",\"32\":\"y\",\"33\":\"y\",\"34\":\"y\",\"35\":\"y\",\"36\":\"y\",\"37\":\"y\",\"38\":\"y\",\"39\":\"y\",\"40\":\"y\",\"41\":\"y\",\"42\":\"y\",\"43\":\"y\",\"44\":\"y\",\"45\":\"y\",\"46\":\"y\",\"47\":\"y\",\"48\":\"y\",\"49\":\"y\"},\"safari\":{\"3.1\":\"n\",\"3.2\":\"n\",\"4\":\"n\",\"5\":\"n\",\"5.1\":\"n\",\"6\":\"n\",\"6.1\":\"n\",\"7\":\"n\",\"7.1\":\"n\",\"8\":\"n\",\"9\":\"y\"},\"opera\":{\"9\":\"n\",\"9.5-9.6\":\"n\",\"10.0-10.1\":\"n\",\"10.5\":\"n\",\"10.6\":\"n\",\"11\":\"n\",\"11.1\":\"n\",\"11.5\":\"n\",\"11.6\":\"n\",\"12\":\"n\",\"12.1\":\"y\",\"15\":\"y\",\"16\":\"y\",\"17\":\"y\",\"18\":\"y\",\"19\":\"y\",\"20\":\"y\",\"21\":\"y\",\"22\":\"y\",\"23\":\"y\",\"24\":\"y\",\"25\":\"y\",\"26\":\"y\",\"27\":\"y\",\"28\":\"y\",\"29\":\"y\",\"30\":\"y\",\"31\":\"y\",\"32\":\"y\