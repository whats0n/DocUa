32-pc/\",\"title\":\"Microsoft Edge support announcement\"}],\"categories\":[\"JS API\",\"Other\"],\"stats\":{\"ie\":{\"5.5\":\"n\",\"6\":\"n\",\"7\":\"n\",\"8\":\"n\",\"9\":\"n\",\"10\":\"n\",\"11\":\"n\"},\"edge\":{\"12\":\"n d #2\",\"13\":\"y\"},\"firefox\":{\"2\":\"n\",\"3\":\"n\",\"3.5\":\"n\",\"3.6\":\"n\",\"4\":\"n\",\"5\":\"n\",\"6\":\"n\",\"7\":\"n\",\"8\":\"n\",\"9\":\"n\",\"10\":\"n\",\"11\":\"n\",\"12\":\"n\",\"13\":\"n\",\"14\":\"n\",\"15\":\"n\",\"16\":\"n\",\"17\":\"n\",\"18\":\"n\",\"19\":\"n\",\"20\":\"n\",\"21\":\"n\",\"22\":\"y\",\"23\":\"y\",\"24\":\"y\",\"25\":\"y\",\"26\":\"y\",\"27\":\"y\",\"28\":\"y\",\"29\":\"y\",\"30\":\"y\",\"31\":\"y\",\"32\":\"y\",\"33\":\"y\",\"34\":\"y\",\"35\":\"y\",\"36\":\"y\",\"37\":\"y\",\"38\":\"y\",\"39\":\"y\",\"40\":\"y\",\"41\":\"y\",\"42\":\"y\",\"43\":\"y\",\"44\":\"y\"},\"chrome\":{\"4\":\"n\",\"5\":\"n\",\"6\":\"n\",\"7\":\"n\",\"8\":\"n\",\"9\":\"n\",\"10\":\"n\",\"11\":\"n\",\"12\":\"n\",\"13\":\"n\",\"14\":\"n\",\"15\":\"n\",\"16\":\"n\",\"17\":\"n\",\"18\":\"n\",\"19\":\"n\",\"20\":\"n\",\"21\":\"n\",\"22\":\"n\",\"23\":\"n\",\"24\":\"n\",\"25\":\"n\",\"26\":\"n\",\"27\":\"n\",\"28\":\"a #1\",\"29\":\"a #1\",\"30\":\"a #1\",\"31\":\"a #1\",\"32\":\"a #1\",\"33\":\"a #1\",\"34\":\"a #1\",\"35\":\"a #1\",\"36\":\"a #1\",\"37\":\"a #1\",\"38\":\"a #1\",\"39\":\"a #1\",\"40\":\"a #1\",\"41\":\"a #1\",\"42\":\"a #1\",\"43\":\"a #1\",\"44\":\"a #1\",\"45\":\"a #1\",\"46\":\"a #1\",\"47\":\"a #1\",\"48\":\"a #1\",\"49\":\"a #1\"},\"safari\":{\"3.1\":\"n\",\"3.2\":\"n\",\"4\":\"n\",\"5\":\"n\",\"5.1\":\"n\",\"6\":\"n\",\"6.1\":\"n\",\"7\":\"n\",\"7.1\":\"n\",\"8\":\"n\",\"9\":\"n\"},\"opera\":{\"9\":\"n\",\"9.5-9.6\":\"n\",\"10.0-10.1\":\"n\",\"10.5\":\"n\",\"10.6\":\"n\",\"11\":\"n\",\"11.1\":\"n\",\"11.5\":\"n\",\"11.6\":\"n\",\"12\":\"n\",\"12.1\":\"n\",\"15\":\"a #1\",\"16\":\"a #1\",\"17\":\"a #1\",\"18\":\"a #1\",\"19\":\"a #1\",\"20\":\"a #1\",\"21\":\"a #1\",\"22\":\"a #1\",\"23\":\"a #1\",\"24\":\"a 